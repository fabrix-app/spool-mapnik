{
  "name": "@fabrix/spool-mapnik",
  "version": "1.6.11",
  "description": "Spool: Mapnik Tile Server with Fabrix",
  "homepage": "https://fabrix.app",
  "author": {
    "name": "scott-wyatt",
    "email": "scottwyatt86@gmail.com",
    "url": "https://cali-style.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabrix-app/spool-mapnik/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabrix-app/spool-mapnik.git"
  },
  "scripts": {
    "build": "tsc -p ./lib/tsconfig.release.json",
    "lint": "tslint -p ./lib",
    "watch": "tsc -w -p ./lib/tsconfig.release.json",
    "test": "npm run clean && npm run lint && npm run build && nyc mocha",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "ci": "cd .. && ci"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "pre-commit": [
    "test"
  ],
  "keywords": [
    "spool",
    "fabrix",
    "trailjs",
    "mapnik",
    "tilelive",
    "mapserver",
    "geoserver",
    "tiles",
    "gis",
    "postgis",
    "gdal",
    "map",
    "wms"
  ],
  "dependencies": {
    "@mapbox/tilelive": "^5.12.6",
    "@mapbox/tilelive-mapnik": "^1.0.0",
    "aws-sdk": "2.814.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@fabrix/fabrix": "^1.6.3",
    "@fabrix/lint": "^1.0.0-alpha.3",
    "@fabrix/spool-i18n": "^1.1.0",
    "@types/node": "~10.3.4",
    "mapnik": "~3.7.0",
    "mocha": "^5",
    "nyc": "^12.0.2",
    "smokesignals": "^3.0.4",
    "supertest": "^3.1.0",
    "tslib": "~1.9.0",
    "tslint": "~5.10.0",
    "tslint-microsoft-contrib": "~5.0.3",
    "tsutils": "~2.27.1",
    "typescript": "~2.8.1"
  },
  "peerDependencies": {
    "@fabrix/fabrix": "^1.6.0",
    "mapnik": "^4.2.1"
  },
  "engines": {
    "node": ">= 7.6.0 =< 8.0.0",
    "npm": ">= 3.10.0"
  }
}
